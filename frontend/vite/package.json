{
  "name": "vite",
  "version": "1.0.0",
  "description": "Development environment for website creation with design and static coding.",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build-vue": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "npm run lint:style && npm run lint:es && npm run lint:html",
    "lint:style": "stylelint src/**/*.{css,scss,vue,ts,tsx}",
    "lint:es": "eslint src/**/*.{vue,ts,tsx}",
    "lint:html": "markuplint src/**/*.{html,hbs,vue,pug,tsx}",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,json,md,html,vue,hbs}' 'dist/**/*.html' --ignore-path .prettierignore"
  },
  "devDependencies": {
    "@markuplint/jsx-parser": "^4.7.19",
    "@markuplint/mustache-parser": "^4.6.19",
    "@markuplint/pug-parser": "^4.6.19",
    "@markuplint/react-spec": "^4.5.19",
    "@markuplint/vue-parser": "^4.6.19",
    "@markuplint/vue-spec": "^4.5.19",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/globule": "^1.1.10",
    "@types/jquery": "^3.5.32",
    "@types/node": "^24.2.1",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@types/three": "^0.179.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "autoprefixer": "^10.4.21",
    "css-declaration-sorter": "^7.2.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-vue": "^10.4.0",
    "globule": "^1.3.4",
    "lil-gui": "^0.20.0",
    "markuplint": "^4.12.0",
    "postcss": "^8.5.6",
    "postcss-html": "^1.8.0",
    "postcss-syntax": "^0.36.2",
    "prettier": "^3.6.2",
    "sass": "^1.90.0",
    "stats.js": "^0.17.0",
    "stylelint": "^16.23.1",
    "stylelint-config-standard-scss": "^15.0.1",
    "stylelint-config-standard-vue": "^1.0.0",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.9.2",
    "vite": "^7.1.1",
    "vite-plugin-glsl": "^1.5.1",
    "vite-plugin-handlebars": "^2.0.0",
    "vite-plugin-pug": "^0.4.1",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.0.5"
  },
  "dependencies": {
    "chart.js": "^4.5.0",
    "gsap": "^3.13.0",
    "jquery": "^3.7.1",
    "pinia": "^3.0.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.0",
    "swiper": "^11.2.10",
    "three": "^0.179.1",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1"
  },
  "browserslist": {
    "production": [
      ">0.5% and not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
