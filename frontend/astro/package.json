{
  "name": "astro",
  "version": "1.0.0",
  "description": "Development environment for website creation with design and static coding.",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "astro dev --port 2000",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview --port 8000",
    "astro": "astro",
    "lint": "npm run lint:style && npm run lint:es && npm run lint:html",
    "lint:style": "stylelint 'src/**/*.{css,scss,astro,vue}'",
    "lint:es": "eslint 'src/**/*.{astro,tsx,ts,vue}'",
    "lint:html": "markuplint 'src/**/*.{html,astro,tsx,vue}'",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,json,md,html,vue,astro}' 'dist/**/*.html' --ignore-path .prettierignore",
    "path": "node relativePath.mjs"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/react": "^4.3.1",
    "@astrojs/sitemap": "^3.5.1",
    "@astrojs/vue": "^5.1.1",
    "@markuplint/astro-parser": "^4.6.21",
    "@markuplint/jsx-parser": "^4.7.21",
    "@markuplint/react-spec": "^4.5.21",
    "@markuplint/vue-parser": "^4.6.21",
    "@markuplint/vue-spec": "^4.5.21",
    "@tailwindcss/vite": "^4.1.13",
    "@types/jquery": "^3.5.33",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "astro": "^5.13.7",
    "autoprefixer": "^10.4.21",
    "css-declaration-sorter": "^7.2.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-vue": "^10.4.0",
    "globals": "^16.4.0",
    "markuplint": "^4.13.1",
    "postcss-html": "^1.8.0",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "sass": "^1.92.1",
    "stylelint": "^16.24.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-standard-scss": "^16.0.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-prettier": "^5.0.3",
    "stylelint-scss": "^6.12.1",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.43.0"
  },
  "dependencies": {
    "jquery": "^3.7.1",
    "pinia": "^3.0.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "vue": "^3.5.21"
  },
  "browserslist": {
    "production": [
      ">0.5% and not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
